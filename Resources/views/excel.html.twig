<div id="phantasos-container" class="row">
    <object data="{{ url('MesdPhantasosClient_getMedia',
       {
           'mediaFileId': info.getFirstOfFormat('text/html').getMediaFileId(),
           'fileName': info.getFirstOfFormat('text/html').getName()
       }) }}" type="text/html" width="{{ width }}" height="{{ height }}">
       <p>
           Your browser does not support viewing this media directly.
           To view download the original uploaded file <a href="{{ url('MesdPhantasosClient_getMedia',
              {
                  'mediaFileId': info.getOriginal().getMediaFileId(),
                  'fileName': info.getOriginal().getName()
              }) }}">here</a>
       </p>
    </object>
</div>
